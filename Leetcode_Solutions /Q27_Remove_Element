class Solution {
    public int removeElement(int[] nums, int val) {
        int index=0;
        for(int i=0;i<nums.length;i++)
        {   // here program enters in if condition only when condition is satisfied and that value stores in nums by using index variable
            if(nums[i]!=val)
            {
                nums[index]=nums[i];
                index++;
            }
        }
        return index;
        
    }
}
